
typedef unsigned char  u8;
typedef unsigned short u16;

void foo(void) {
    
    int a;
    a = _ioreg(0x1B, "PORTA");
    _ioreg(0x1B, "PORTA") = a+1;
    
    _ioreg(0x1B, "PORTA") |= 16;
    _ioreg(0x1B, "PORTA") |= 42;
    _ioreg(0x1B, "PORTA") |= a;
    
    _ioreg(0x1B, "PORTA") &= ~16;
    _ioreg(0x1B, "PORTA") &= 42;
    _ioreg(0x1B, "PORTA") &= a;
    
    
}